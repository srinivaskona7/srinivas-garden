    text : "SAPUI5"
}));
oShell.attachWorksetItemSelected(function(oEvent) {
    var key = oEvent.getParameter("key");
    setUrlHash(key);
});

function setUrlHash(key) {
    // #home shall point to url without hash
    if (normalizeContentKey(key) == "home") {
        window.location.hash = "";
    } else {
        window.location.hash = key;
    }
}

var home_content_html = new sap.ui.core.HTML("home_content");
var cloud_tools_content_html = new sap.ui.core.HTML("cloud_tools_content");
var abap_tools_content_html = new sap.ui.core.HTML("abap_tools_content");
var bw_tools_content_html = new sap.ui.core.HTML("bw_tools_content");
var sapui5_tools_content_html = new sap.ui.core.HTML("sapui5_tools_content");
var hana_tools_content_html = new sap.ui.core.HTML("hana_tools_content");
var hci_tools_content_html = new sap.ui.core.HTML("hci_tools_content");
var idm_tools_content_html = new sap.ui.core.HTML("idm_tools_content");
var mobile_tools_content_html = new sap.ui.core.HTML("mobile_tools_content");

// comment required by legacy SDK versions
// document.cookie = "eula_3_2_agreed=tools.hana.ondemand.com/developer-license-3_2.txt;"

var eulaConst = {}
eulaConst.devLicense={}
eulaConst.devLicense.cookieName = 'eula_3_2_agreed'
eulaConst.devLicense.cookieValue = 'tools.hana.ondemand.com/developer-license-3_2.txt'
eulaConst.devLicense.eulaDialogHeaderElement = 'eula-dialog-header'
eulaConst.devLicense.eulaTextElement = 'eula-text'

eulaConst.sqlmigLicense={}
eulaConst.sqlmigLicense.cookieName = 'eula_sqlmigration_license_20211130_agreed'
eulaConst.sqlmigLicense.cookieValue = 'tools.hana.ondemand.com/sap-sql-migration-tool-agreement-2021-11-30.txt'
eulaConst.sqlmigLicense.eulaDialogHeaderElement = 'sqlmigLicense-eula-dialog-header'
eulaConst.sqlmigLicense.eulaTextElement = 'sqlmigLicense-eula-text'

function getEulaMappingForURL(downloadURL){
    var eulaMappings = eulaConst.devLicense;
    if (downloadURL.match('additional/sapdbmtk-')){
        eulaMappings = eulaConst.sqlmigLicense;
    }
    return eulaMappings;
}

function isEulaAgreed(urlToOpenAfterAgree) {
    var eulaMappings = getEulaMappingForURL(urlToOpenAfterAgree);
    if (isEulaCookiePresent(eulaMappings)) {
        return true;
    } else {
        // the EULA dialog opens asynchronously, so we need to open the link after the user has agreed
        openEulaDialog(urlToOpenAfterAgree, eulaMappings);
        // disable default event behaviour -> the href is not followed
        return false;
    }
}

function openEulaDialog(urlToOpenAfterAgree, eulaMappings) {
    var dialogTitle = "You are about to download '" + getFileNameFromURL(urlToOpenAfterAgree) + "'";

    var oDialogContent = getEulaLayout(eulaMappings);

    var dialogButtons = new Array(new sap.ui.commons.Button({
        text : getAgreeButtonText(),
        press : function() {
            acceptedEulaDialog(urlToOpenAfterAgree, eulaMappings);
            oDialog.close();
        }
    }), new sap.ui.commons.Button({
        text : getRejectButtonText(),
        press : function() {
            oDialog.close();
        }
    }));

    var oDialog = new sap.ui.commons.Dialog({
        width : "85%",
        height : "85%",
        resizable : false,
        modal : true,
        title : dialogTitle,
        content : oDialogContent,
        buttons : dialogButtons
    });
    oDialog.attachClosed( function() {
        onDialogClose();
    });
    oDialog.setInitialFocus(oDialogContent);
    oDialog.open();
}

function onDialogClose() {
    var showEulaForDownload = jQuery.sap.getUriParameters().get("showEulaForDownload");
    if (showEulaForDownload != null) {
        window.location.replace('.');
    } else {
        setContent(normalizeContentKey(window.location.hash));
    }
}

function acceptedEulaDialog(urlToOpen, eulaMappings) {
    createEulaCookie(90, eulaMappings);
    window.open(urlToOpen, '_blank');
}

function getEulaLayout(eulaMappings) {
    return new sap.ui.commons.layout.BorderLayout({
        width : "100%",
        height : "100%",
        top : new sap.ui.commons.layout.BorderLayoutArea({
            contentAlign : "left",
            visible : true,
            content : [ getEulaDialogHeader(eulaMappings) ]
        }),
        center : new sap.ui.commons.layout.BorderLayoutArea({
            contentAlign : "left",
            visible : true,
            content : [ getEulaDialogBody("100%", "100%", eulaMappings) ]
        })
    });
}

function getEulaDialogHeader(eulaMappings) {
    return new sap.ui.core.HTML({
        content : getContentFromElement(eulaMappings.eulaDialogHeaderElement)
    });
}

function getEulaDialogBody(width, height, eulaMappings) {
    return new sap.ui.commons.TextArea({
        value : getContentFromElement(eulaMappings.eulaTextElement),
        width : width,
        height : height,
        editable : false
    });
}

function createEulaCookie(nrOfDays, eulaMappings) {
    var expiryDate = new Date();
    expiryDate.setDate(expiryDate.getDate() + nrOfDays);
    document.cookie = eulaMappings.cookieName + "=" + eulaMappings.cookieValue + "; path=/; expires=" + expiryDate.toGMTString() + ";";
}

function isEulaCookiePresent(eulaMappings) {
    return readCookie(eulaMappings.cookieName) == eulaMappings.cookieValue;
}

function setShellContent(key) {
    var key = normalizeContentKey(key);
    parts = key.split("-");
    setContent(parts[0]);
    setSelectedWorksetItem(parts[0]);

    setTimeout(function() {
        if (document.getElementById(key)) {
            document.getElementById(key).scrollIntoView();
        }
    }, 1);

    var showEulaForDownload = jQuery.sap.getUriParameters().get("showEulaForDownload");
    if (showEulaForDownload != null) {
        setTimeout(function() {
            openEulaDialog(showEulaForDownload, getEulaMappingForURL(showEulaForDownload));
        }, 2);
    }
}

function normalizeContentKey(key) {
    // remove # from hashtags
    key = key.replace("#", "");
    parts = key.split("-");
    switch (parts[0]) {
    case "home":
    case "hanatools":
    case "cloud":
    case "abap":
    case "bw":
    case "sapui5":
    case "cloudintegration":
    case "idm":
    case "mobile":
        return key;
        // #tools, #sdk, #optional are required for compatibility of existing bookmarks
    case "tools":
    case "sdk":
    case "optional":
        return "cloud";
    default:
        return "home";
    }
}

function setContent(tabKey) {
    switch (tabKey) {
    case "home":
    default:
        setAndShowContent(home_content_html);
        break;
    case "cloud":
        setAndShowContent(cloud_tools_content_html);
        break;
    case "abap":
        setAndShowContent(abap_tools_content_html);
        break;
    case "bw":
        setAndShowContent(bw_tools_content_html);
        break;
    case "sapui5":
        setAndShowContent(sapui5_tools_content_html);
        break;
    case "hanatools":
        setAndShowContent(hana_tools_content_html);
        break;
    case "cloudintegration":
        setAndShowContent(hci_tools_content_html);
        break;
    case "idm":
        setAndShowContent(idm_tools_content_html);
        break;
    case "mobile":
        setAndShowContent(mobile_tools_content_html);
        break;
    }
}

function setAndShowContent(oControl) {
    // Show initiallly hidden elements.
    // Elements are initially hidden to avoid flickering on page load with Firefox
    $("#" + oControl.getId()).css("display", "");
    oShell.setContent(oControl);
}

function setSelectedWorksetItem(tabKey) {
    var items = oShell.getWorksetItems();
    for ( var i = 0; i < items.length; i++) {
        if (items[i].getKey() == tabKey) {
            oShell.setSelectedWorksetItem(items[i].getId());
            break;
        }
    }
}

// enable back/forward and links to tabs
jQuery(window).bind("hashchange", function(e) {
    setShellContent(window.location.hash);
});

oShell.placeAt("shellArea");
jQuery(document).ready(function() {
    // DOM must be ready before we can manipulate it
    setShellContent(window.location.hash);
    var nowDate = new Date();
    jQuery.each ($(".oldVersionHcpSdk"), function (index,element) {
        var releaseDateAttribute = element.getAttribute("releaseDate");
        var releaseDate = new Date(releaseDateAttribute);
        var dayDiff =((nowDate.getTime() - releaseDate.getTime()) / (1000 * 60 * 60 * 24));
        if (releaseDateAttribute !== null && dayDiff > 3 * 30){
            var oldComment = element.cells[1];
            var deprecationMsg = "Deprecated";
            if (oldComment.innerHTML.indexOf(deprecationMsg)!=0){
                if (oldComment.innerHTML.length>0){
                    $(oldComment).html(deprecationMsg + ". " + oldComment.innerHTML);
                }else{
                    $(oldComment).html(deprecationMsg);
                }
            }
        }
        if (releaseDateAttribute !== null && dayDiff > 15 * 30){
            $(element).remove();
        }
    });
});

function createOldVersionsButton(selector) {
    var buttonText = "Show old versions";
    var oldVersionsButton = new sap.ui.commons.Button({
        text : buttonText,
        press : function() {
            // jQuery toggle() and is(":visible") does not work in IE8
            var elem = $(selector)[0];
            if (elem.style.display == 'none' || elem.style.display == '') {
                $(selector).show();
                oldVersionsButton.setText("Hide old versions");
            } else {
                $(selector).hide();
                oldVersionsButton.setText(buttonText);
            }
        }
    });
    return oldVersionsButton;
}

createOldVersionsButton(".oldVersionHcpSdk").placeAt("toggleOldVersionsHcpSdk");
createOldVersionsButton(".oldVersionSapui5").placeAt("toggleOldVersionsSapui5");
createOldVersionsButton(".oldVersionCpcli").placeAt("toggleOldVersionsCpcli");

function createSubcomponentsButton(selector, subselector) {
    var buttonText = "Show Subcomponents";
    var subcomponentsButton = new sap.ui.commons.Button({
        text : buttonText,
        press : function() {
            // jQuery toggle() and is(":visible") does not work in IE8
            var elem = $(subselector)[0];
            if (elem.style.display == 'none' || elem.style.display == '') {
                $(selector).hide();
                $(subselector).show();
                subcomponentsButton.setText("Hide Subcomponents");
            } else {
                $(selector).show();
                $(subselector).hide();
                subcomponentsButton.setText(buttonText);
            }
        }
    });
    return subcomponentsButton;
}

createSubcomponentsButton(".componentsHanaClients", ".subcomponentsHanaClients").placeAt("toggleSubcomponentsHanaClients");

